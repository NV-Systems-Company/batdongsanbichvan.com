<!-- BEGIN: main -->
<div class="viewcat">
	<div class="cat-head">
		<h1>{CAT_INFO.title}</h1>
		<div class="cat-info">
			<!-- BEGIN: image -->
			<img src="{CAT_INFO.image}" width="200px" class="img-thumbnail image pull-left" />
			<!-- END: image -->

			<!-- BEGIN: description -->
			<p>{CAT_INFO.description_html}</p>
			<!-- END: description -->

			<div class="clearfix"></div>
		</div>
	</div>
	<div class="cat-data">{DATA}</div>
	<!-- BEGIN: page -->
	<div class="text-center clear">{PAGE}</div>
	<!-- END: page -->
</div>
<!-- END: main -->